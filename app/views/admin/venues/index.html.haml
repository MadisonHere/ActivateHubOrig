%h2.main-title Venues

#admin-content
  %table.index-table.venue-duplicates
    %thead
      %tr
        %th Title
        %th # Events
        %th Address
    %tbody
      - @venues.each do |venue|
        %tr
          %td= link_to venue.title, venue_path(venue)
          %td= venue.events_count || '-'
          %td= venue.full_address || venue.address
